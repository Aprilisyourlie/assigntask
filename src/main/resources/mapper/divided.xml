<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wx.assigntask.dao.DividedDao" >

    <!--插入划分组-->
    <insert id="insertDivided" parameterType="com.wx.assigntask.entity.Divided" >
        <selectKey resultType="java.lang.Integer" order="AFTER" keyProperty="id">
            select @@IDENTITY  as dividedid
        </selectKey>
        INSERT INTO divided (alg1name,alg2name) VALUES (#{param1},#{param2})
    </insert>

    <select id="findDividedIdByAlgs" resultType="int">
        SELECT dividedid FROM divided WHERE alg2name = #{param1} AND alg2name = #{param2}
    </select>



</mapper>
