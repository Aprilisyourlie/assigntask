<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wx.assigntask.dao.SubTaskDao" >

    <!--获取任务原始数据-->
    <select id="findSubBySubId"  resultType="com.wx.assigntask.entity.Divided">
        SELECT * FROM sub_task WHERE subtask_id = ${param1}
    </select>

    <!--插入数据-->
    <insert id="insertSubTask" parameterType="com.wx.assigntask.entity.Divided">
        INSERT INTO sub_task (algorithm_id1,algorithm_id2,judged_frequency) VALUES (${param1},${param2},${param3})
    </insert>

</mapper>
