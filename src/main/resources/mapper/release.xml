<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wx.assigntask.dao.ReleaseDao" >

    <select id="finAlgsByInputName" parameterType="String" resultType="String" >
        SELECT alg FROM `release` WHERE inputname=#{inputName}
    </select>

    <update id="updateDivided" parameterType="com.wx.assigntask.entity.Release">
        UPDATE `release` SET ifdivided = #{param1} WHERE inputname = #{param2} and alg = #{param3}
    </update>

    <select id="findDivided" resultType="String">
        SELECT ifdivided FROM `release` WHERE  inputname = #{param1} AND alg = #{param2}
    </select>

</mapper>
